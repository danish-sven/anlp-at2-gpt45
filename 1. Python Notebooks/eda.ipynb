   "source": [
    "# create the pos tags for original sentences and name the column 'o_pos_tags'\n",
    "df_cor_eda['o_pos_tags']=df_cor_eda['original'].apply(lambda x: nltk.pos_tag(word_tokenize(x)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# count the number of verb, noun, adjective, adverb, pronoun, preposition, conjection, article, modal in original sentences\n",
    "df_cor_eda['o_num_verbs'] = df_cor_eda['o_pos_tags'].apply(lambda x: len([word for word, pos in x if pos.startswith('V')]))\n",
    "df_cor_eda['o_num_nouns'] = df_cor_eda['o_pos_tags'].apply(lambda x: len([word for word, pos in x if pos.startswith('N')]))\n",
